   <%- include('../partials/navbar'); %>

   <main role="main" class="container">
      <div class="card mt-3">
         <div class="card-body text-center">
            <div class="row">
               <div class="col-12">
                  <form>
                     <div class="form-group">
                        <label for="distanceRange">Distanza : <b><span id="distance"></span> KM</b></label>
                        <input type="range" class="form-control-range" id="distanceRange">
                     </div>
                  </form>
               </div>
               <div class="col-sm-12 col-md-8 col-lg-8">
                  <div id="usersmap" style="height:700px;"></div>
               </div>
               <div class="col-sm-12 col-md-4 col-lg-4">
                  <ul class="list-group" id="usersList" style="overflow-y: scroll;height: 700px;">
                  </ul>
               </div>
            </div>

         </div>
      </div>

   </main>

   <%- include('../partials/footer'); %>

   <!-- Custom users map loading -->
   <script type="text/javascript" src="/js/usersMap.js"></script>
